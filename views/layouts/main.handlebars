  <div class="modalButton">
    {{!-- Need Modal trigger --}}
    <a class="waves-effect waves-light btn modal-trigger" href="#modal-need">Need?</a>
    {{!-- Deed Modal trigger --}}
    <a class="waves-effect waves-light btn modal-trigger" href="#modal-deed">Deed!</a>
  </div>


  <!-- Modal Structure -->

  {{!-- Need Modal Form --}}
  <div id="modal-need" class="modal">
    <div class="modal-content">
      <h4>Help someone in Need</h4>
      <div class="row">
        <form class="col s12">
          <div class="row">
            <div class="input-field col s6">
              <input placeholder="Placeholder" id="first_name" type="text" class="validate">
              <label for="first_name">First Name</label>
            </div>
            <div class="input-field col s6">
              <input id="last_name" type="text" class="validate">
              <label for="last_name">Last Name</label>
            </div>
          </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input id="cell" type="email" class="validate">
          <label for="cell">Cell #</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input id="zip" type="email" class="validate">
          <label for="zip">Zipcode</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <select>
            <option value="" disabled selected>Choose your option</option>
            <option value="1">Public</option>
            <option value="2">Private</option>
          </select>
          <label>Select Privacy Type</label>
          <span class="helper-text" data-error="wrong" data-success="right">*Private will not list your need in a
            search result. A link will be sent to your email that you can share to your family and friends for your
            need.</span>
          <div class="modal-footer">
            <a href="#!" class="modal-close waves-effect waves-green btn-flat">Agree</a>
          </div>
        </div>
      </div>
    </div>
    </form>
  </div>
  </div>
  </div>
  </div>

  {{!-- Deed Modal Form --}}
  <div id="modal-deed" class="modal">
    <div class="modal-content">
      <h4>Do a deed!</h4>
      <div class="row">
        <div class="input-field col s12">
          <input id="deedID" type="email" class="validate">
          <label for="deedID"></label>
        </div>
        <p>If you have a deed # from a friend or family member enter above, if not press continue</p>
      </div>
      <div class="modal-footer">
        <a href="#!" class="modal-close waves-effect waves-green btn-flat">Continue</a>
      </div>
    </div>
    <br><br>
  </div>

